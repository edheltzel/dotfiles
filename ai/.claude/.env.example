# PAI Environment Configuration
# Copy to .env and fill in your actual values

DA=Atlas
TIME_ZONE=America/New_York

# TTS Provider (google or elevenlabs)
TTS_PROVIDER=elevenlabs

# Google Cloud TTS (if using google provider)
GOOGLE_API_KEY=your_google_api_key_here

# ElevenLabs (if using elevenlabs provider)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_DEFAULT=your_default_voice_id

# Voice personalities (optional - get IDs from ElevenLabs dashboard)
ELEVENLABS_VOICE_ENGINEER=voice_id_here
ELEVENLABS_VOICE_PENTESTER=voice_id_here
ELEVENLABS_VOICE_INTERN=voice_id_here
ELEVENLABS_VOICE_ARCHITECT=voice_id_here
ELEVENLABS_VOICE_RESEARCHER=voice_id_here
ELEVENLABS_VOICE_ARTIST=voice_id_here
ELEVENLABS_VOICE_DESIGNER=voice_id_here
ELEVENLABS_VOICE_WRITER=voice_id_here
