        "orange": "#EBBCBA",
        "path": "#9CCFD8",
        "pink": "#EB6F92",
        "purple": "#C4A7E7",
        "red": "#EB6F92",
        "success": "#31748F",
        "white": "#E0DEF4",
        "yellow": "#F6C177"
      },
      "tokyo-night": {
        "blue": "#7AA2F7",
        "branch": "#BB9AF7",
        "cyan": "#7DCFFF",
        "error": "#F7768E",
        "git_added": "#9ECE6A",
        "git_ahead": "#C0CAF5",
        "git_deleted": "#F7768E",
        "git_modified": "#E0AF68",
        "git_staged": "#9ECE6A",
        "git_untracked": "#7DCFFF",
        "green": "#9ECE6A",
        "magenta": "#BB9AF7",
        "muted": "#565F89",
        "orange": "#FF9E64",
        "path": "#7DCFFF",
        "pink": "#FF007C",
        "purple": "#BB9AF7",
        "red": "#F7768E",
        "success": "#9ECE6A",
        "white": "#C0CAF5",
        "yellow": "#E0AF68"
      },
      "rose-pine-moon": {
        "blue": "#9CCFD8",
        "branch": "#C4A7E7",
        "cyan": "#9CCFD8",
        "error": "#EB6F92",
        "git_added": "#3E8FB0",
        "git_ahead": "#E0DEF4",
        "git_deleted": "#EB6F92",
        "git_modified": "#F6C177",
        "git_staged": "#3E8FB0",
        "git_untracked": "#9CCFD8",
        "green": "#3E8FB0",
        "magenta": "#C4A7E7",
        "muted": "#6E6A86",
        "orange": "#EA9A97",
        "path": "#9CCFD8",
        "pink": "#EB6F92",
        "purple": "#C4A7E7",
        "red": "#EB6F92",
        "success": "#3E8FB0",
        "white": "#E0DEF4",
        "yellow": "#F6C177"
      },
      "tokyo-night-moon": {
        "blue": "#82AAFF",
        "branch": "#C099FF",
        "cyan": "#86E1FC",
        "error": "#FF757F",
        "git_added": "#C3E88D",
        "git_ahead": "#C8D3F5",
        "git_deleted": "#FF757F",
        "git_modified": "#FFC777",
        "git_staged": "#C3E88D",
        "git_untracked": "#86E1FC",
        "green": "#C3E88D",
