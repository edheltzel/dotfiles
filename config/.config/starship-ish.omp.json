{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"var": {
		"lightning": "⚡"
	},
	"palettes": {
		"list": {
			"eldritch": {
				"blue": "#04D1F9",
				"branch": "#F265B5",
				"cyan": "#04D1F9",
				"error": "#F16C75",
				"git_added": "#37F499",
				"git_ahead": "#EBFAFA",
				"git_deleted": "#F16C75",
				"git_modified": "#F1FC79",
				"git_staged": "#37F499",
				"git_untracked": "#04D1F9",
				"green": "#37F499",
				"magenta": "#A48CF2",
				"muted": "#7081BE",
				"orange": "#F7C67F",
				"path": "#04D1F9",
				"pink": "#F265B5",
				"purple": "#A48CF2",
				"red": "#F16C75",
				"success": "#37F499",
				"white": "#EBFAFA",
				"yellow": "#F1FC79"
			},
			"rose-pine": {
				"blue": "#9CCFD8",
				"branch": "#C4A7E7",
				"cyan": "#9CCFD8",
				"error": "#EB6F92",
				"git_added": "#31748F",
				"git_ahead": "#E0DEF4",
				"git_deleted": "#EB6F92",
				"git_modified": "#F6C177",
				"git_staged": "#31748F",
				"git_untracked": "#9CCFD8",
				"green": "#31748F",
				"magenta": "#C4A7E7",
				"muted": "#6E6A86",
				"orange": "#EBBCBA",
				"path": "#9CCFD8",
				"pink": "#EB6F92",
				"purple": "#C4A7E7",
				"red": "#EB6F92",
				"success": "#31748F",
				"white": "#E0DEF4",
				"yellow": "#F6C177"
			},
			"tokyo-night": {
				"blue": "#7AA2F7",
				"branch": "#BB9AF7",
				"cyan": "#7DCFFF",
				"error": "#F7768E",
				"git_added": "#9ECE6A",
				"git_ahead": "#C0CAF5",
				"git_deleted": "#F7768E",
				"git_modified": "#E0AF68",
				"git_staged": "#9ECE6A",
				"git_untracked": "#7DCFFF",
				"green": "#9ECE6A",
				"magenta": "#BB9AF7",
				"muted": "#565F89",
				"orange": "#FF9E64",
				"path": "#7DCFFF",
				"pink": "#FF007C",
				"purple": "#BB9AF7",
				"red": "#F7768E",
				"success": "#9ECE6A",
				"white": "#C0CAF5",
				"yellow": "#E0AF68"
			}
		},
		"template": "eldritch"
	},
	"transient_prompt": {
		"template": "❯ ",
		"foreground": "p:success",
		"background": "transparent"
	},
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"options": {
						"cache_duration": "none",
						"folder_icon": "\u2026",
						"hide_root_location": true,
						"style": "agnoster_short"
					},
					"template": "<p:blue>{{ .Var.lightning }}</>{{ .Path }} ",
					"foreground": "p:path",
					"type": "path",
					"style": "plain"
				},
				{
					"options": {
						"branch_icon": "\uf418 ",
						"cache_duration": "none",
						"fetch_e_count": true,
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"template": "<p:muted>on</> {{ .HEAD }}{{if .BranchStatus }} <p:git_ahead>{{ .BranchStatus }}</>{{ end }}{{ if .Working.Changed }} <p:git_modified>\uf448</>{{ if gt .Working.Added 0 }} <p:git_added>+{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Modified 0 }} <p:git_modified>~{{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Deleted 0 }} <p:git_deleted>-{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Untracked 0 }} <p:git_untracked>?{{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <p:git_staged>\uf046</>{{ if gt .Staging.Added 0 }} <p:git_added>+{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Modified 0 }} <p:git_modified>~{{ .Staging.Modified }}</>{{ end }}{{ if gt .Staging.Deleted 0 }} <p:git_deleted>-{{ .Staging.Deleted }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }} <p:purple>\uf420 {{ .StashCount }}</>{{ end }} ",
					"foreground": "p:branch",
					"type": "git",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\uf270 {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
					"foreground": "p:orange",
					"type": "aws",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "{{if .Version}} \udb80\udfd7 {{.Version}} {{end}}",
					"foreground": "p:orange",
					"type": "project",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none",
						"extensions": ["deno.json", "deno.jsonc", "deno.lock"]
					},
					"template": "\udb84\udf62 {{ .Full }} ",
					"foreground": "p:muted",
					"type": "deno",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\udb80\udf99 {{ .Full }} ",
					"foreground": "p:green",
					"type": "node",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none",
						"extensions": ["package-lock.json"]
					},
					"template": "\ue71e {{ .Full }} ",
					"foreground": "p:red",
					"type": "npm",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none",
						"extensions": ["pnpm-lock.yaml"]
					},
					"template": "\udb84\uddda {{ .Full }} ",
					"foreground": "p:yellow",
					"type": "pnpm",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none",
						"fetch_version": true
					},
					"template": "\ue28d {{ .Full }} ",
					"foreground": "p:white",
					"type": "bun",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\uf1a0 {{.Full}} ",
					"foreground": "p:cyan",
					"type": "go",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\ue620 {{.Full}} ",
					"foreground": "p:blue",
					"type": "lua",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\ue608 {{.Full}} ",
					"foreground": "p:purple",
					"type": "php",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\ue606 {{.Full}} ",
					"foreground": "p:yellow",
					"type": "python",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\udb83\udd2d {{.Full}} ",
					"foreground": "p:red",
					"type": "ruby",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "\ue68b {{.Full}} ",
					"foreground": "p:orange",
					"type": "rust",
					"style": "plain"
				},
				{
					"options": {
						"cache_duration": "none",
						"style": "roundrock",
						"threshold": 0
					},
					"template": "<p:muted>\udb81\udd1b</> {{ .FormattedMs }} ",
					"foreground": "p:muted",
					"type": "executiontime",
					"style": "plain"
				}
			],
			"newline": true
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"options": {
						"cache_duration": "none"
					},
					"template": "❯ ",
					"type": "text",
					"style": "plain",
					"foreground_templates": [
						"{{ if gt .Code 0 }}p:error{{ end }}",
						"{{ if eq .Code 0 }}p:success{{ end }}"
					]
				}
			],
			"newline": true
		}
	],
	"version": 4
}
