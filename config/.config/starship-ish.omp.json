      }
    },
    "template": "rose-pine-moon"
  },
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "p:success",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "folder_icon": "…",
            "hide_root_location": true,
            "style": "agnoster_short"
          },
          "template": "<p:blue>{{ .Var.lightning }}</>{{ .Path }} ",
          "foreground": "p:path",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "branch_icon": " ",
            "cache_duration": "none",
            "fetch_e_count": true,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "<p:muted>on</> {{ .HEAD }}{{if .BranchStatus }} <p:git_ahead>{{ .BranchStatus }}</>{{ end }}{{ if .Working.Changed }} <p:git_modified></>{{ if gt .Working.Added 0 }} <p:git_added>+{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Modified 0 }} <p:git_modified>~{{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Deleted 0 }} <p:git_deleted>-{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Untracked 0 }} <p:git_untracked>?{{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <p:git_staged></>{{ if gt .Staging.Added 0 }} <p:git_added>+{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Modified 0 }} <p:git_modified>~{{ .Staging.Modified }}</>{{ end }}{{ if gt .Staging.Deleted 0 }} <p:git_deleted>-{{ .Staging.Deleted }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }} <p:purple> {{ .StashCount }}</>{{ end }} ",
          "foreground": "p:branch",
          "type": "git",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "p:orange",
          "type": "aws",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{if .Version}} 󰏗 {{.Version}} {{end}}",
          "foreground": "p:orange",
          "type": "project",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "extensions": [
              "deno.json",
              "deno.jsonc",
              "deno.lock"
            ]
          },
          "template": "󱍢 {{ .Full }} ",
          "foreground": "p:muted",
          "type": "deno",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "󰎙 {{ .Full }} ",
          "foreground": "p:green",
          "type": "node",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "extensions": [
              "package-lock.json"
            ]
          },
          "template": " {{ .Full }} ",
          "foreground": "p:red",
          "type": "npm",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "extensions": [
              "pnpm-lock.yaml"
            ]
          },
          "template": "󱇚 {{ .Full }} ",
          "foreground": "p:yellow",
          "type": "pnpm",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "template": " {{ .Full }} ",
          "foreground": "p:white",
          "type": "bun",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Full}} ",
          "foreground": "p:cyan",
          "type": "go",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Full}} ",
          "foreground": "p:blue",
          "type": "lua",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Full}} ",
          "foreground": "p:purple",
          "type": "php",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Full}} ",
          "foreground": "p:yellow",
          "type": "python",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "󰴭 {{.Full}} ",
          "foreground": "p:red",
          "type": "ruby",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " {{.Full}} ",
          "foreground": "p:orange",
          "type": "rust",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "template": "<p:muted>󰔛</> {{ .FormattedMs }} ",
          "foreground": "p:muted",
          "type": "executiontime",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "❯ ",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:error{{ end }}",
            "{{ if eq .Code 0 }}p:success{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4
}
