{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": " {{ .Folder }} :: {{if .Root}}Admin{{end}}",
  "palette": {
    "main-bg": "#24283b",
    "terminal-red": "#f7768e",
    "pistachio-green": "#9ece6a",
    "terminal-green": "#73daca",
    "terminal-yellow": "#e0af68",
    "terminal-blue": "#7aa2f7",
    "celeste-blue": "#b4f9f8",
    "light-sky-blue": "#7dcfff",
    "terminal-white": "#c0caf5",
    "white-blue": "#a9b1d6",
    "blue-bell": "#9aa5ce",
    "pastel-grey": "#cfc9c2",
    "terminal-magenta": "#bb9af7",
    "blue-black": "#565f89",
    "terminal-black": "#414868",
    "t-background": "p:main-bg"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:terminal-blue",
          "template": "\u279c "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:terminal-magenta",
          "properties": {
            "style": "folder"
          },
          "template": "<b>{{ .Path }}</b> <p:light-sky-blue>\u26a1</>"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:terminal-red{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0)}}p:light-sky-blue {{ end }}",
            "{{ if gt .Ahead 0 }}p:terminal-blue{{ end }}",
            "{{ if gt .Behind 0 }}p:celeste-blue{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<p:terminal-yellow> {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<p:pistachio-green> {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "branch_icon": "\uf418 "
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": " \uf00d"
        },
        {
          "foreground": "p:pastel-grey",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "style": "diamond",
          "template": " \udb81\udd1b {{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "aws",
          "style": "plain",
          "foreground": "p:terminal-yellow",
          "template": "\uf270 {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        },
        {
          "type": "npm",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": "\ue616 {{ .Full }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:pistachio-green",
          "template": "\ue718 {{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:light-sky-blue",
          "template": "\uf1a0 {{ .Full}} "
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "p:terminal-blue",
          "template": "\ue608 {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:terminal-yellow",
          "template": "\ue606 {{ .Full }} "
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:terminal-red",
          "template": "\udb83\udd2d {{ .Full}} "
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:terminal-yellow",
          "template": "îš‹ {{ .Full}} "
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:terminal-white",
          "properties": {
            "time_format": "\ue641 03:04 PM"
          },
          "template": "{{ .CurrentDate | date .Format }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pistachio-green",
          "style": "plain",
          "template": "\u25b6",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:terminal-blue",
    "template": "\u279c "
  },
  "transient_prompt": {
    "background": "p:t-background",
    "foreground": "p:terminal-blue",
    "template": "\u279c "
  },
  "final_space": true,
  "version": 2,
  "terminal_background": "p:t-background"
}
