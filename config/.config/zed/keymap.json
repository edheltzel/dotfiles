[
	// Workspace
	{
		"context": "Workspace",
		"bindings": {
			"ctrl-alt-cmd-x": "zed::Extensions",
			"ctrl-alt-cmd-g": "git_panel::ToggleFocus",
			"ctrl-alt-cmd-e": "project_panel::ToggleFocus",
			"ctrl-alt-cmd-d": "debug_panel::ToggleFocus",
			"ctrl-alt-cmd-b": "workspace::ToggleLeftDock",
			"alt-`": "workspace::NewCenterTerminal",
			// Tabs/Buffers
			"cmd-1": ["pane::ActivateItem", 0],
			"cmd-2": ["pane::ActivateItem", 1],
			"cmd-3": ["pane::ActivateItem", 2],
			"cmd-4": ["pane::ActivateItem", 3],
			"cmd-5": ["pane::ActivateItem", 4],
			"cmd-6": ["pane::ActivateItem", 5],
			"cmd-7": ["pane::ActivateItem", 6],
			"cmd-8": ["pane::ActivateItem", 7],
			"cmd-9": ["pane::ActivateItem", 8],
		},
	},
	// Window navigation
	{
		"context": "Dock || Terminal || Editor",
		"bindings": {
			"ctrl-h": "workspace::ActivatePaneLeft",
			"ctrl-l": "workspace::ActivatePaneRight",
			"ctrl-k": "workspace::ActivatePaneUp",
			"ctrl-j": "workspace::ActivatePaneDown",
		},
	},
	//Git Panel
	{
		"context": "GitPanel && !editing",
		"bindings": {
			"ctrl-alt-cmd-g": "workspace::ToggleRightDock",
		},
	},
	// Debug Panel
	{
		"context": "DebugPanel",
		"bindings": {
			"ctrl-alt-cmd-d": "workspace::ToggleBottomDock",
		},
	},
	//Project Panel
	{
		"context": "ProjectPanel && !editing",
		"bindings": {
			"ctrl-alt-cmd-e": "workspace::ToggleRightDock",
			"space e": "workspace::ToggleRightDock",
			"r": "project_panel::Rename",
			"a": "project_panel::NewFile",
		},
	},
	// Terminal
	{
		"context": "Terminal",
		"bindings": {
			"cmd-k \\": "pane::SplitRight",
			"cmd-k -": "pane::SplitDown",
		},
	},
	// Tasks + Spawn AI agents
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space g g": "git_panel::ToggleFocus",
			"space a c": [
				"task::Spawn",
				{
					"task_name": "Claude Danger",
					"reveal_target": "center",
				},
			],
			"ctrl-alt-cmd-c": [
				"task::Spawn",
				{
					"task_name": "Claude Danger",
					"reveal_target": "center",
				},
			],
			"ctrl-alt-cmd-o": [
				"task::Spawn",
				{
					"task_name": "Opencode",
					"reveal_target": "center",
				},
			],
			"space a o": [
				"task::Spawn",
				{
					"task_name": "Opencode",
					"reveal_target": "center",
				},
			],
			"ctrl-alt-cmd-s": [
				"task::Spawn",
				{
					"task_name": "Sidecar",
					"reveal_target": "center",
				},
			],
			"space a s": [
				"task::Spawn",
				{
					"task_name": "Sidecar",
					"reveal_target": "center",
				},
			],
		},
	},
	// Editor + Vim
	// LazyVim like keybindings
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			// File / Find
			// "space space": "file_finder::Toggle",
			"space f f": "file_finder::Toggle",
			"space f r": "file_finder::Toggle",
			"space f s": "workspace::Save",
			"space f S": "workspace::SaveAll",
			"space f n": "workspace::NewFile",
			// Buffers
			"space f b": "tab_switcher::ToggleAll",
			"space b b": "tab_switcher::Toggle",
			"space ,": "tab_switcher::ToggleAll",
			"space b a": "pane::CloseAllItems",
			"space b o": "pane::CloseOtherItems",
			"space b d": "pane::CloseActiveItem",
			"space w d": "pane::CloseActiveItem",
			"space \\": "pane::SplitAndMoveRight",
			"space b h": "pane::SplitAndMoveLeft",
			"space b j": "pane::SplitAndMoveDown",
			"space b k": "pane::SplitAndMoveUp",
			"space b l": "pane::SplitAndMoveRight",
			// Search
			"space s g": "pane::DeploySearch",
			"space /": "pane::DeploySearch",
			"space s r": "buffer_search::DeployReplace",
			"space s s": "outline::Toggle",
			"space s S": "project_symbols::Toggle",
			// LSP / Code
			"space c a": "editor::ToggleCodeActions",
			"space c r": "editor::Rename",
			"space c f": "editor::Format",
			"space c i": "editor::OrganizeImports",
			"space c d": "editor::GoToDiagnostic",
			// Git
			"space g b": "editor::ToggleGitBlameInline",
			"space g d": "git::Diff",
			// UI Toggles
			"space u z": "workspace::ToggleCenteredLayout",
			"space u w": "editor::ToggleSoftWrap",
			"space u l": "editor::ToggleLineNumbers",
			"space u L": "editor::ToggleRelativeLineNumbers",
			"space u g": "editor::ToggleIndentGuides",
			"space u h": "editor::ToggleInlayHints",
			"space u d": "editor::ToggleInlineDiagnostics",
			// Window
			"space |": "pane::SplitRight",
			"space -": "pane::SplitDown",
			"space w m": "workspace::ToggleZoom",
			"space w z": "workspace::ToggleZoom",
			// Terminal
			"space f t": "workspace::NewCenterTerminal",
			// Diagnostics (trouble.nvim equivalent)
			"space x x": "diagnostics::Deploy",
			// Clear search highlights
			"space n h": ["workspace::SendKeystrokes", ": n o h enter"],
			// Panels
			"space e": "project_panel::ToggleFocus",
			"space o": "outline_panel::ToggleFocus",
			// AI Agents
			"space c g": ["agent::NewExternalAgentThread", { "agent": "gemini" }],
			"space c c": ["agent::NewExternalAgentThread", { "agent": "claude_code" }],
			"space c z": ["agent::NewExternalAgentThread", { "agent": "native_agent" }],
			"space c o": [
				"agent::NewExternalAgentThread",
				{
					"agent": {
						"custom": {
							"name": "OpenCode",
							"command": {
								"command": "opencode",
								"args": ["acp"],
							},
						},
					},
				},
			],
			// Misc
			"space q q": "workspace::CloseWindow",
			"space k m": "zed::OpenKeymap",
			"cmd-k": "command_palette::Toggle",
			"cmd shift l": "editor::SelectAllMatches",
		},
	},
	// Vim Motions - shared normal + visual
	{
		"context": "vim_mode == normal || vim_mode == visual",
		"bindings": {
			"g h": "editor::MoveToBeginningOfLine",
			"g l": "editor::MoveToEndOfLine",
			"cmd-shift-d": "editor::DuplicateLineDown",
			"ctrl-alt-d": "editor::DuplicateLineDown",
			"alt-k": "editor::MoveLineUp",
			"alt-j": "editor::MoveLineDown",
			"cmd-enter": "editor::Fold",
			"enter": "editor::ToggleFold",
			// LSP navigation
			"g d": "editor::GoToDefinition",
			"g D": "editor::GoToTypeDefinition",
			"g I": "editor::GoToImplementation",
			"g r": "editor::FindAllReferences",
			"K": "editor::Hover",
			// Diagnostics
			"] d": "editor::GoToDiagnostic",
			"[ d": "editor::GoToPreviousDiagnostic",
			// Git hunks
			"] h": "editor::GoToHunk",
			"[ h": "editor::GoToPreviousHunk",
			// Buffer cycling
			"shift-h": "pane::ActivatePreviousItem",
			"shift-l": "pane::ActivateNextItem",
			// Folding
			"z R": "editor::UnfoldAll",
			"z M": "editor::FoldAll",
			// Redo
			"U": "editor::Redo",
		},
	},
	// Sneak - normal mode only (S conflicts with surround in visual)
	{
		"context": "vim_mode == normal",
		"bindings": {
			"s": "vim::PushSneak",
			"S": "vim::PushSneakBackward",
			"space j j": "vim::PushSneak",
		},
	},
	// Subword motions
	{
		"context": "VimControl && !menu && vim_mode != operator",
		"bindings": {
			"w": "vim::NextSubwordStart",
			"b": "vim::PreviousSubwordStart",
			"e": "vim::NextSubwordEnd",
			"g e": "vim::PreviousSubwordEnd",
		},
	},
	// Any bracket/quote text objects (mini.ai style)
	{
		"context": "vim_operator == a || vim_operator == i || vim_operator == cs || vim_operator == ds || vim_operator == ys",
		"bindings": {
			"q": "vim::MiniQuotes",
			"b": "vim::MiniBrackets",
			"Q": "vim::AnyQuotes",
			"B": "vim::AnyBrackets",
		},
	},
	// Surround in visual mode (S" or gsa")
	{
		"context": "vim_mode == visual",
		"bindings": {
			"shift-s": "vim::PushAddSurrounds",
			"g s a": "vim::PushAddSurrounds",
		},
	},
	// Insert mode - jk to escape
	{
		"context": "vim_mode == insert",
		"bindings": {
			"j k": "vim::NormalBefore",
		},
	},
]
