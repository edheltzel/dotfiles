{
	"[css][scss][less]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"[html][django-html][handlebars][hbs][mustache][jinja][jinja-html][nj][njk][nunjucks][twig]": {
		"editor.defaultFormatter": "monosans.djlint"
	},
	"[javascript][javascriptreact][vue]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json][jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[markdown]": {
		"editor.formatOnPaste": true,
		"editor.formatOnSave": true,
		"editor.wordWrap": "bounded",
		"editor.wordWrapColumn": 100
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"[typescript][typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	// Extensions Settings
	// "accessibility.dimUnfocused.enabled": true,
	// "accessibility.dimUnfocused.opacity": 0.6,
	"chat.commandCenter.enabled": false,
	// CSpell
	"cSpell.customDictionaries": {
		"custom-global-dictionary": {
			"description": "Custom Global Dictionary",
			"name": "Custom Global Dictionary",
			"path": "/Users/ed/.dotfiles/local/.local/share/cspell/global-dictionary.txt"
		}
	},
	// Catppuccin Icons
	"catppuccin-icons.associations.folders": {
		"_flightdeck": "folder_workflows",
		"_includes": "folder_templates",
		"_static": "folder_constant",
		"collections": "folder_content",
		"partials": "folder_composables"
	},
	"catppuccin-icons.hidesExplorerArrows": true,
	// Color Highlight
	"color-highlight.markRuler": false,
	// Comments
	"comments.openView": "never",
	"comments.visible": false,
	"css.format.spaceAroundSelectorSeparator": true,
	// Diff Editor
	"diffEditor.experimental.showMoves": true,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	// DJLint
	"djlint.pythonPath": "/Users/ed/.local/pipx/venvs/djlint/bin/python",
	"djlint.useVenv": false,
	// Find It Faster
	"find-it-faster.general.useTerminalInEditor": true,
	// Core Editor Settings
	"editor.accessibilitySupport": "off",
	"editor.bracketPairC7lorization.enabled": true,
	"editor.codeActionsOnSave": {
		"quickfix.biome": "always"
	},
	"editor.cursorSmoothCaretAnimation": "on",
	// "editor.cursorSurroundingLines": 99,
	"editor.cursorBlinking": "phase",
	"editor.cursorSurroundingLinesStyle": "all",
	"editor.cursorWidth": 3,
	"editor.foldingStrategy": "indentation",
	"editor.fontFamily": "Lilex, Lilex Nerd Font Mono, Dank Mono, Symbols Nerd Font",
	"editor.fontLigatures": true,
	"editor.fontSize": 19,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.formatOnType": false,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": "active",
	"editor.hover.delay": 500,
	"editor.inlineSuggest.enabled": true,
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.insertSpaces": false,
	"editor.language.colorizedBracketPairs": [
		[
			"[",
			"]"
		],
		[
			"(",
			")"
		],
		[
			"{",
			"}"
		],
		[
			"{%",
			"%}"
		],
		[
			"{{",
			"}}"
		]
	],
	"editor.letterSpacing": 0.5,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.overviewRulerBorder": false,
	"editor.pasteAs.enabled": false,
	"editor.rulers": [
		{
			"color": "#f7c67f10",
			"column": 80
		},
		{
			"color": "#f7c67f15",
			"column": 100
		},
		{
			"color": "#f7c67f20",
			"column": 120
		},
		{
			"color": "#f7c67f25",
			"column": 160
		}
	],
	"editor.smoothScrolling": true,
	"editor.snippetSuggestions": "top",
	"editor.stickyScroll.enabled": true,
	"editor.stickyScroll.maxLineCount": 3,
	"editor.stickyScroll.scrollWithEditor": false,
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 100,
	"workbench.activityBar.location": "bottom",
	"workbench.colorTheme": "Eldritch",
	"workbench.colorCustomizations": {
		"[Eldritch]": {
			"cursorColumnColor": "#37f49910",
			"editor.background": "#18192B",
			"editorGutter.background": "#18192B",
			"editorStickyScroll.background": "#0F1013",
		}
	},
	"workbench.editor.editorActionsLocation": "hidden",
	"workbench.editor.pinnedTabSizing": "shrink",
	"workbench.editor.showIcons": false,
	"workbench.editor.showTabs": "none",
	"workbench.iconTheme": "catppuccin-latte",
	"catppuccin.syncWithIconPack": false,
	"workbench.layoutControl.enabled": false,
	"workbench.secondarySideBar.showLabels": false,
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"workbench.tips.enabled": false,
	"workbench.tree.expandMode": "doubleClick",
	"workbench.tree.indent": 20,
	"workbench.tree.stickyScrollMaxItemCount": 4,
	// Window Settings
	"window.commandCenter": false,
	"window.density.editorTabHeight": "compact",
	"window.title": "${dirty}${separator}${rootName}${separator}${profileName}",
	// Files Settings
	"files.associations": {
		"*.svg": "html",
		"*.webc": "nunjucks",
		"_redirects": "plaintext",
		"*.h": "c"
	},
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 15000,
	"files.insertFinalNewline": true,
	"files.simpleDialog.enable": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	// Search Settings
	"search.exclude": {
		"**/*.code-search": true,
		"**/.*cache": true,
		"**/node_modules": true,
		"*lock*": true,
		".DS_Store": true
	},
	"search.showLineNumbers": true,
	"search.useIgnoreFiles": true,
	// Terminal Settings
	"terminal.external.osxExec": "wezterm.app",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "block",
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.env.osx": {
		"GIT_EDITOR_CONTEXT": "code"
	},
	"terminal.integrated.fontFamily": "Lilex, Lilex Nerd Font Mono",
	"terminal.integrated.fontSize": 17,
	"terminal.integrated.fontWeightBold": "400",
	"terminal.integrated.scrollback": 10000,
	"terminal.integrated.suggest.enabled": true,
	// Git Settings
	"git.autoStash": true,
	"git.confirmSync": false,
	"git.enableCommitSigning": true,
	"git.enableSmartCommit": true,
	"git.ignoreRebaseWarning": true,
	"git.openRepositoryInParentFolders": "never",
	"github.gitProtocol": "ssh",
	// Language Features
	"javascript.format.semicolons": "insert",
	"javascript.preferences.quoteStyle": "double",
	"javascript.suggest.completeFunctionCalls": true,
	"javascript.suggest.paths": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"js/ts.implicitProjectConfig.checkJs": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.preferences.quoteStyle": "double",
	"typescript.suggest.paths": false,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"html.format.templating": true,
	// Emmet
	"emmet.excludeLanguages": [],
	"emmet.includeLanguages": {
		"astro": "html",
		"blade": "html",
		"liquid": "html",
		"md": "html",
		"njk": "html",
		"scss": "css",
		"webc": "html"
	},
	"emmet.syntaxProfiles": {
		"liquid": "html",
		"md": "html",
		"njk": "html",
		"scss": "css"
	},
	// Error Lens
	"errorLens.enabledDiagnosticLevels": [
		"error",
		"info",
		"warning"
	],
	"errorLens.fontSize": "18",
	"errorLens.fontStyleItalic": true,
	// Explorer
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	// Project Manager
	"projectManager.sortList": "Saved",
	"projectManager.groupList": true,
	"projectManager.showParentFolderInfoOnDuplicates": true,
	"projectManager.showProjectNameInStatusBar": false,
	"projectManager.projectsLocation": "~/Library/Application Support/Code/User/customStuff",
	"projectManager.tags": [
		"Personal",
		"Work",
		"Flightdeck",
		"VSCode",
		"OSS"
	],
	"projectManager.git.baseFolders": [
		"~/.dotfiles",
		"~/Developer/",
		"~/Sites/"
	],
	// GitLens
	"gitlens.ai.experimental.model": "vscode",
	"gitlens.hovers.currentLine.details": false,
	"gitlens.views.commitDetails.files.layout": "tree",
	// Merge Conflict
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	// Path IntelliSense
	"path-intellisense.showHiddenFiles": true,
	// Security
	"security.workspace.trust.enabled": false,
	// SuperMaven
	"supermaven.enable": {
		"*": true
	},
	"supermaven.enableFixWithSupermaven": true,
	"supermaven.enableJumpSuggestions": false,
	// Tailwind CSS
	"tailwindCSS.emmetCompletions": true,
	// TOML
	"evenBetterToml.formatter.columnWidth": 120,
	// VIM
	"vim.changeWordIncludesWhitespace": true,
	"vim.cursorStylePerMode.insert": "line",
	"vim.cursorStylePerMode.normal": "block-outline",
	"vim.easymotion": true,
	"vim.highlightedyank.color": "#f265b530",
	"vim.highlightedyank.enable": true,
	"vim.textwidth": 100,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"args": {
				"after": [
					"y",
					"y"
				]
			},
			"command": "vim.remap",
			"keys": "y",
			"name": "Copy/Yank line",
			"type": "command"
		},
		{
			"before": [
				"<space>"
			],
			"commands": [
				"vspacecode.space"
			]
		},
		{
			"before": [
				""
			],
			"commands": [
				"vspacecode.space",
				{
					"args": "m",
					"command": "whichkey.triggerKey"
				}
			]
		},
		{
			"before": [
				"g",
				"h"
			],
			"commands": [
				"cursorHome"
			]
		},
		{
			"before": [
				"g",
				"l"
			],
			"commands": [
				"cursorEnd"
			]
		}
	],
	"vim.smartRelativeLine": true,
	"vim.useSystemClipboard": true,
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<space>"
			],
			"commands": [
				"vspacecode.space"
			]
		},
		{
			"before": [
				","
			],
			"commands": [
				"vspacecode.space",
				{
					"args": "m",
					"command": "whichkey.triggerKey"
				}
			]
		},
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		}
	],
	// VSCode Custom CSS
	"vscode_custom_css.imports": [
		"file://${userHome}/Library/Application Support/Code/User/customStuff/vscode-styles.css",
		"file://${userHome}/Library/Application Support/Code/User/customStuff/vscode-scripts.js"
	],
	"vscode_custom_css.statusbar": false,
	// VSpaceCode
	"vspacecode.bindingOverrides": [
		{
			"command": "workbench.action.quickOpen",
			"icon": "file",
			"keys": "o",
			"name": "Go To File...",
			"position": 1,
			"type": "command"
		},
		{
			"command": "workbench.action.showAllEditorsByMostRecentlyUsed",
			"icon": "files",
			"keys": ",",
			"name": "Show all Buffers",
			"position": 3,
			"type": "command"
		},
		{
			"command": "editor.action.revealDefination",
			"icon": "symbol-class",
			"keys": "j.d",
			"name": "Go to Definition",
			"type": "command"
		},
		{
			"command": "workbench.action.toggleSidebarVisibility",
			"icon": "symbol-tree",
			"keys": "e.e",
			"name": "Toggle Sidebar",
			"type": "command"
		},
		{
			"command": "editor.action.goToReferences",
			"icon": "symbol-enum",
			"keys": "j.r",
			"name": "Go to Referrence",
			"type": "command"
		},
		{
			"command": "workbench.action.gotoLine",
			"icon": "symbol-number",
			"keys": "j.L",
			"name": "Jump to line number",
			"telemetry.telemetryLevel": "crash",
			"type": "command"
		},
		{
			"command": "file-browser.open",
			"icon": "file",
			"keys": "f.F",
			"name": "Open File/Folder...",
			"position": 2,
			"type": "command"
		},
		{
			"command": "workbench.action.closeAllEditors",
			"icon": "clear-all",
			"keys": "b.o",
			"name": "Close all buffers",
			"position": 26,
			"type": "command"
		},
		{
			"command": "workbench.action.closeEditorsInGroup",
			"icon": "clear-all",
			"keys": "b.G",
			"name": "Close buffers in Group",
			"position": 27,
			"type": "command"
		},
		{
			"command": "workbench.action.toggleAuxiliaryBar",
			"icon": "layout-sidebar-left",
			"keys": "T.B",
			"name": "Toggle Secondary Side Bar Visibility",
			"position": 1,
			"type": "command"
		},
		{
			"keys": "f.t",
			"position": "-1"
		},
		{
			"command": "workbench.view.explorer",
			"icon": "list-tree",
			"keys": "f.e",
			"name": "Toggle File Explorer",
			"position": "7",
			"type": "command"
		},
		{
			"command": "lazygit.openLazygit",
			"icon": "git-pull-request-closed",
			"keys": "g.g",
			"name": "Open Lazygit",
			"position": "0",
			"type": "command"
		},
	],
}
